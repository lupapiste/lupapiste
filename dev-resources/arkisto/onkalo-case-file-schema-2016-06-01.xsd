	<!-- Asiakirjallisen tiedon käsittelyn metatiedot (CaseFile)                       -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="CaseFile">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="lp:Subject" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Created" maxOccurs="unbounded"/>
			<xs:element ref="lp:Issued" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Sent" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Modified" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Available" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Acquired" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:NativeId"/>
			<xs:element ref="lp:OtherId" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Language" maxOccurs="unbounded"/>
			<xs:element ref="lp:Description" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Abstract" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Restriction"/>
			<xs:element ref="lp:Title"/>
			<xs:element ref="lp:CaseFileRelation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:RetentionPeriod"/>
			<xs:element ref="lp:RetentionReason"/>
			<xs:element ref="lp:RetentionPeriodEnd" minOccurs="0"/>
			<xs:element ref="lp:Status"/>
			<xs:element ref="lp:ClassificationScheme"/>
			<xs:element ref="lp:Function"/>
			<xs:element ref="lp:Agent" maxOccurs="unbounded"/>
			<xs:element ref="lp:Type" minOccurs="0"/>
			<xs:element ref="lp:Finished" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:ElectronicNotification" minOccurs="0"/>
			<xs:element ref="lp:Action" maxOccurs="unbounded"/>
			<xs:element ref="lp:Custom" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	</xs:element>
