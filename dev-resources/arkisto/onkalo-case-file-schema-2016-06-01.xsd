	<!-- Asiakirjan metatiedot (Record)                                                        -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="Record" type="lp:RecordType">
	</xs:element>
	<xs:complexType name="RecordType">
		<xs:sequence>
			<xs:element ref="lp:Created" maxOccurs="unbounded"/>
			<xs:element ref="lp:NativeId"/>
			<xs:element ref="lp:Description" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="lp:Title"/>
			<xs:element ref="lp:Type"/>
			<xs:element ref="lp:Version" minOccurs="0"/>
			<xs:element ref="lp:Agent" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
