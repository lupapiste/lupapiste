<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ymy="http://www.kuntatietopalvelu.fi/gml/ymparisto/ymparistoluvat" xmlns:gml="http://www.opengis.net/gml" xmlns:yht="http://www.kuntatietopalvelu.fi/gml/yhteiset" targetNamespace="http://www.kuntatietopalvelu.fi/gml/ymparisto/ymparistoluvat" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.2.3">
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd"/>
	<xs:import namespace="http://www.kuntatietopalvelu.fi/gml/yhteiset" schemaLocation="http://www.kuntatietopalvelu.fi/gml/yhteiset/2.1.8/yhteiset.xsd"/>
	<!--Local usage-->
	<!--<xs:import namespace="http://www.opengis.net/gml" schemaLocation="..\..\yhteiset\gml_dummy.xsd"/>
	<xs:import namespace="http://www.kuntatietopalvelu.fi/gml/yhteiset" schemaLocation="..\..\yhteiset\yhteiset.xsd"/>-->
	<!-- ================================================= -->
	<!-- Package: <<XSDschema>> Ympäristöluvat -->
	<!-- ================================================= -->
	<!-- Kohdejoukko: Ympäristöluvat -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<FeatureType>> Ympäristöluvat-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="Ymparistoluvat" type="ymy:YmparistoluvatType" substitutionGroup="gml:_Feature">
		<xs:annotation>
			<xs:documentation>Skeeman juurielementti</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="YmparistolupaType">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="kasittelytietotieto" type="ymy:KasittelyHistoriaType" maxOccurs="unbounded"/>
					<xs:element name="luvanTunnistetiedot" type="yht:YmparistoLuvanTunnisteTiedotType" minOccurs="0"/>
					<xs:element name="valvontatapahtumattieto" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Valvontatapahtuma" type="yht:ValvontatapahtumaType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="paatostieto" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Paatos" type="yht:YmparistoPaatosType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="lausuntotieto" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Lausunto" type="yht:LausuntoYmpType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="maksajatieto" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Maksaja" type="yht:MaksajaType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="hakija" type="yht:YhteystietoType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="toiminta" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TOIMINTA, JOLLE LUPAA HAETAAN</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="peruste" type="xs:string">
									<xs:annotation>
										<xs:documentation>Hakijan käsitys luvan haun perusteista</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="kuvaus" type="xs:string">
									<xs:annotation>
										<xs:documentation>Lyhyt kuvaus toiminnasta</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="luvanHakemisenSyy" type="ymy:LuvanHakemisenSyyType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="laitoksentiedot" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Laitoksen/toimipaikantiedot</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Laitos" type="ymy:LaitosType">
									<xs:annotation>
										<xs:documentation>Laitos/toimipaikka jolle lupaa haetaan</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="voimassaOlevatLuvat" type="ymy:VoimassaOlevatLuvatType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>VOIMASSA OLEVAT YMPÄRISTÖLUPA-, VESILUPA- TAI MUUT PÄÄTÖKSET JA SOPIMUKSET</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="alueJaYmparisto" type="ymy:AlueJaYmparistoType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TIEDOT KIINTEISTÖISTÄ JA NIILLÄ SIJAITSEVISTA LAITOKSISTA JA TOIMINNOISTA SEKÄ NÄIDEN HALTIJOISTA
YHTEYSTIETOINEEN</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="toiminnanSijaintitieto" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TIEDOT TOIMINNAN SIJAINTIPAIKASTA, YMPÄRISTÖOLOSUHTEISTA, YMPÄRISTÖN LAADUSTA JA ASUTUKSESTA SEKÄ
SELVITYS ALUEEN KAAVOITUSTILANTEESTA</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ToiminnanSijainti" type="ymy:ToiminnanSijaintiType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="tuotantotiedot" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Tuotanto" type="ymy:TuotantoType">
									<xs:annotation>
										<xs:documentation>(V)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="raakaAineet" type="ymy:RaakaAineetType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Raaka-aineet, kemikaalit, polttoaineet ja muut tuotantoon käytettävät aineet, niiden varastointi
ja säilytys sekä kulutus ja veden käyttö (V)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="energiaTehokkuus" type="ymy:EnergiaTehokkuusType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Esitetään tiedot käytettävästä energiasta ja arvio sen käytön tehokkuudesta (V)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="vesihuolto" type="ymy:VesihuoltoType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vedenhankinta ja viemäröinti (V)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ymparistoriskit" type="yht:YmparistoLiiteType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Arvio toimintaan liittyvistä ympäristöriskeistä, onnettomuuksien estämiseksi suunnitelluista
toimista sekä toimista häiriötilanteissa</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="liikennejarjestelyt" type="yht:YmparistoLiiteType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Kuvataan laitoksen toiminnasta aiheutuvan liikenteen määrä</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ymparistoHallintajarjestelma" type="ymy:YmparistoHallintajarjestelmaType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>kuvaus mahdollisesta ympäristöasioiden hallintajärjestelmästä</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ymparistokuormitustieto" minOccurs="0">
						<xs:annotation>
							<xs:documentation>tiedot toiminnan päästöjen laadusta ja määrästä</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Ymparistokuormitus" type="ymy:YmparistokuormitusType">
									<xs:annotation>
										<xs:documentation>tiedot toiminnan päästöjen laadusta ja määrästä</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="referenssiPiste" type="gml:PointPropertyType" minOccurs="0"/>
					<xs:element name="koontiKentta" type="xs:string" minOccurs="0"/>
					<xs:element name="liitetieto" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Liite" type="yht:YmparistoLiiteType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="asianKuvaus" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RaakaAineetType">
		<xs:sequence>
			<xs:element name="RaakaAineet">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="raakaAinetieto" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RaakaAine" type="ymy:RaakaAineType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="polttoainetieto" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Polttoaine" type="ymy:PolttoaineType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="muualtaTulevattieto" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Tiedot muualta tulevista, toiminnassa raaka- tai polttoaineina käytettävistä
jätteistä sekä se prosessi, jossa jätettä käytetään.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="MuualtaTulevat" type="ymy:MuualtaTulevatType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Tiedot muualta tulevista, toiminnassa raaka- tai polttoaineina käytettävistä
jätteistä sekä se prosessi, jossa jätettä käytetään.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="kemikaalittieto" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Kemikaalit" type="ymy:KemikaalitType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="varastoalue" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="varastointipaikkatieto" type="ymy:VarastointipaikkatietoType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>varastointipaikat ja ongelmajätteiden varastot merkitään karttapiirrokseen, joka
liitetään hakemukseen</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="sijaintitieto" type="yht:SijaintitietoType"/>
									<xs:element name="kuvaus" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="vedenKaytto" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Tiedot ilmoitetaan
keskimääräisen toteutuneen toimintavuoden ajalta ja hakemuksen mukaisen toiminnan
osalta</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="raakavedenMaara" type="xs:string">
										<xs:annotation>
											<xs:documentation>raakaveden vuotuinen määrä</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="jaahdytysvesi" type="xs:string">
										<xs:annotation>
											<xs:documentation>osuus %</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="prosessivesi" type="xs:string">
										<xs:annotation>
											<xs:documentation>osuus %</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="kierratettavaVesi" type="xs:string">
										<xs:annotation>
											<xs:documentation>osuus %:na kokonaisvesimääräst</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KemikaalitType">
		<xs:sequence>
			<xs:element name="vuosi" type="xs:string">
				<xs:annotation>
					<xs:documentation>Vuosi jota tiedot koskevat</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kemikaalitieto" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Kemikaali" type="ymy:KemikaaliType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sijaintitieto" type="yht:SijaintitietoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Käyttöpakan sijainti</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VarastointipaikkaType">
		<xs:complexContent>
			<xs:extension base="yht:AbstractPaikkatietopalveluKohde">
				<xs:sequence>
					<xs:element name="sijaintitieto" type="yht:SijaintitietoType"/>
					<xs:element name="kuvaus" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VoimassaOlevatLuvatType">
		<xs:sequence>
			<xs:element name="luvat" type="ymy:LuvatType" minOccurs="0"/>
			<xs:element name="vakuutukset" type="ymy:VakuutuksetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mahdollinen ympäristövahinkovakuutus (vakuutusyhtiö ja vakuutuksen numero)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AlueJaYmparistoType">
		<xs:sequence>
			<xs:element name="kiinteistonLaitokset" type="ymy:KiinteistonLaitoksetType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Laitokset kiinteistöittäin</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ToiminnanSijaintiType">
		<xs:complexContent>
			<xs:extension base="yht:AbstractPaikkatietopalveluKohde">
				<xs:sequence>
					<xs:element name="sijaintitieto" type="yht:SijaintitietoType" maxOccurs="unbounded"/>
					<xs:element name="ymparistoolosuhteet" type="yht:YmparistoLiiteType" minOccurs="0"/>
					<xs:element name="ymparistonLaatu" type="yht:YmparistoLiiteType" minOccurs="0"/>
					<xs:element name="asutus" type="yht:YmparistoLiiteType" minOccurs="0"/>
					<xs:element name="kaavoitustilanne" type="yht:YmparistoLiiteType" minOccurs="0"/>
					<xs:element name="rajanaapurit" type="ymy:RajanaapuritType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>SELVITYS TOIMINNAN SIJAINTIPAIKAN RAJANAAPUREISTA SEKÄ MUISTA MAHDOLLISISTA ASIANOSAISISTA, JOITA
TOIMINTA JA SEN VAIKUTUKSET ERITYISESTI SAATTAVAT KOSKEA</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TuotantoType">
		<xs:sequence>
			<xs:element name="tuotteet" type="ymy:TuotteetType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="energianTuotanto" type="ymy:EnergianTuotantoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Energian tuotannosta ilmoitetaan seuraavat tiedot keskimääräiseltä toteutuneelta toimintavuodelta,
hakemuksen mukaisesta sekä suurimman mahdollisen tuotantokapasiteetin mukaisesta toiminnasta</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EnergiaTehokkuusType">
		<xs:sequence>
			<xs:element name="kaytettyEnergia" type="ymy:KaytettyEnergiaType" minOccurs="0"/>
			<xs:element name="kaytonTehokkuus" type="ymy:KaytonTehokkuusType">
				<xs:annotation>
					<xs:documentation>Energian käytön tehokkuudesta esitetään</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VesihuoltoType">
		<xs:sequence>
			<xs:element name="Vedenottopaikka" type="ymy:VedenottopaikkaType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
									Vedenottopaikka
								</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Jatevesi" type="ymy:JatevesiType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Jätevesien käsittely </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="YmparistoHallintajarjestelmaType">
		<xs:sequence>
			<xs:element name="nimi" type="xs:string">
				<xs:annotation>
					<xs:documentation>järjestelmän nimi</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kayttoonottoPvm" type="xs:date"/>
			<xs:element name="viimeisinAuditointiPvm" type="xs:date">
				<xs:annotation>
					<xs:documentation>viimeisimmän
ulkoisen auditoinnin ajankohta</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="YmparistokuormitusType">
		<xs:sequence>
			<xs:element name="jatevesimaara" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="yksikko">
							<xs:annotation>
								<xs:documentation>Ilmoituksessa käytetty yksikkö</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="m3/a"/>
									<xs:enumeration value="m3/d"/>
									<xs:enumeration value="ei tiedossa"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="prosessivesi" type="xs:string"/>
						<xs:element name="saniteettivesi" type="xs:string"/>
						<xs:element name="jatevesi" type="xs:string"/>
						<xs:element name="jaahdytysvesi" type="xs:string"/>
						<xs:element name="hulevesi" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="aine" type="ymy:AineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Esitetään ainekohtaisesti kokonaiskuormitus</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kemikaalittieto" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Kemikaalit" type="ymy:KemikaalitType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="paastotIlmaan" type="ymy:PaastotIlmaanType">
				<xs:annotation>
					<xs:documentation>Esitetään päästöt ilmaan</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paastotMaaperaan" type="ymy:PaastotMaaperaanType">
				<xs:annotation>
					<xs:documentation>Pilaantuneen maa-aineksen käsittely</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="melupaastot" type="ymy:MelupaastotType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>esitetään melu- ja tärinäselvitys, jossa esitetään tarvittaessa tiedot toiminnasta aiheutuvasta
melusta ja tärinästä</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paastojenVahentaminen" type="ymy:PaastojenVahentaminenType">
				<xs:annotation>
					<xs:documentation>Päästöjen vähentämistä ja puhdistamista koskevat toimet</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jatteet" type="ymy:JatteetType">
				<xs:annotation>
					<xs:documentation>Syntyvät jätteet ja niiden ominaisuudet ja määrät, jätteiden varastointi sekä edelleen
toimittaminen (V)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jatteidenHyodyntaminen" type="ymy:JatteidenHyodyntaminenType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Esitetään selvitys tehdyistä (esim. viimeisten viiden vuoden ajalta) ja suunnitelluista toimista jätteiden
määrän tai niiden haitallisuuden vähentämiseksi ja jätteiden hyödyntämisestä</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bat" type="yht:YmparistoLiiteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Esitetään arvio parhaan käyttökelpoisen tekniikan (Best Available Techniques, BAT) soveltamisesta.
Parhaalla käyttökelpoisella tekniikalla tarkoitetaan mahdollisimman tehokkaita ja kehittyneitä, teknisesti
ja taloudellisesti toteuttamiskelpoisia tuotanto- ja puhdistusmenetelmiä ja suunnittelu-, rakentamis-,
ylläpito- sekä käyttötapoja, joilla voidaan ehkäistä toiminnan aiheuttama ympäristön pilaantuminen
tai tehokkaimmin vähentää sitä</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ristikkaisvaikutukset" type="yht:YmparistoLiiteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lupahakemusta laadittaessa on otettava huomioon toiminnan vaikutus ympäristöön kokonaisuutena,
pilaantumisen ehkäisemiseksi tarkoitettujen toimien merkitys ympäristön kokonaisuuden kannalta sekä
tekniset ja taloudelliset mahdollisuudet toteuttaa nämä toimet.Tarkastelussa tulisi arvioida mm. ympäristön kannalta erisuuntaisten vaikutusten eli ns. ristikkäisvaikutusten
(cross-media effects) keskinäistä merkittävyyttä</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bep" type="yht:YmparistoLiiteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ympäristön kannalta parhaalla käytännöllä (Best Environmental Practice, BEP) ymmärretään YSL:n
mukaan pilaantumisen ehkäisemiseksi tarkoituksenmukaisia kustannustehokkaita eri toimien yhdistelmiä,
kuten työmenetelmiä sekä raaka-aine- ja polttoainevalintoja</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vaikutuksetYmparistoon" type="ymy:VaikutuksetYmparistoonType">
				<xs:annotation>
					<xs:documentation>Esitetään selvitys tai arvio toiminnan tai laitoksen eri vaikutuksista ympäristöön. Ympäristövaikutukset
esitetään kokonaisvaltaisesti sekä ilmaan, veteen ja maaperään kohdistuvien vaikutusten että melun
ja tärinän suhteen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tarkkailuJaRaportointi" type="ymy:TarkkailuJaRaportointiType">
				<xs:annotation>
					<xs:documentation>Hakemukseen liitetään tarkkailusuunnitelma, jossa esitetään tiedot toiminnan käyttötarkkailusta, ympäristöön
kohdistuvien päästöjen ja niiden vaikutusten tarkkailusta sekä käytettävistä mittausmenetelmistä
ja -laitteista, laskentamenetelmistä ja niiden laadunvarmistuksesta. Esitetään, miten, milloin
ja mille tahoille tuloksista raportoidaan</xs:documentation>
				</xs:annotation>
			</xs:element>
