							<xs:sequence>
								<xs:element name="Osapuoli" type="yak:YleisenAlueenOsapuoliType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="vastuuhenkilotieto" minOccurs="0" maxOccurs="unbounded">
