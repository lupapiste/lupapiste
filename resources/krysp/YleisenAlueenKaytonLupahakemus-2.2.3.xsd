							<xs:sequence>
								<xs:element name="Sijainti" type="yht:SijaintiType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="merkinnatJaPiirroksettieto" minOccurs="0" maxOccurs="unbounded">
