							<xs:sequence>
								<xs:element name="Vastuuhenkilo" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="sukunimi" type="xs:string"/>
											<xs:element name="etunimi" type="xs:string"/>
