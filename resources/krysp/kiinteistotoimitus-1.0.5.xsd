									<xs:element name="kiinteistotunnus" type="yht:KiinteistotunnusType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="maaraAlatieto" minOccurs="0" maxOccurs="unbounded">
