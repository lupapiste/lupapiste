								<xs:enumeration value="KKO:n päätös"/>
								<xs:enumeration value="Valitus lääninoikeuteen"/>
								<xs:enumeration value="Lääninoikeuden päätös"/>
								<xs:enumeration value="Valitus KHO:lle"/>
