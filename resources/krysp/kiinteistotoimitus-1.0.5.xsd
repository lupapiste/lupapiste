				</xs:complexType>
			</xs:element>
			<xs:element name="liitetieto" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Liite" type="yht:LiiteType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="kiinteistotieto" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Kiinteisto">
							<xs:complexType>
