								<xs:element name="Kiinteisto">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="kiinteistotunnus" type="yht:KiinteistotunnusType"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="maaraAlatieto" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="MaaraAla">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="maaraAlatunnus">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="(([0-9]{3}){2}([0-9]{4}){2}[MT][0-9]{4})"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
